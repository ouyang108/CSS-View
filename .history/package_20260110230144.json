    "commit": "czg"
  },
  "commitizen": {
    "path": "node_modules/cz-git"
  },
  "dependencies": {
    "vue": "^3.5.25",
    "vue3-toastify": "^0.2.8",
    "webext-bridge": "^6.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@wxt-dev/module-vue": "^1.0.3",
    "cz-git": "^1.12.0",
    "czg": "^1.12.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.8",
    "wxt": "^0.20.6"
